<?xml version="1.0" encoding="utf-8"?>

<!-- # Scenario: SLAM -->

<!-- # tf
map
    gmapping
odom
    ekf imu + odom
base_link
-->

<launch>

    <!-- # move_base -->
    <include file="$(find my_move_base)/launch/move_base.launch">
